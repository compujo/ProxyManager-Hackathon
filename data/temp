<script language="javascript">
function searchboxFocus1() {
	/*	If the site search textbox has a value of search when the user places focus on it, the
		value will be cleared out.  Finally, the text font color in the textbox is verified set
		to black (as opposed to a diminished black).												*/
	if (document.getElementById('criteria1').value == 'Search') {
		document.getElementById('criteria1').value			= '';
	}
	document.getElementById('formFocused1').value			= '1';	
	document.getElementById('criteria1').style.color		= '#000';
}

function searchboxBlur1() {
	/*	When the user focus departs from the site search textbox, the value and text color will
		only be initialized if the value of the textbox is basically null (blank).					*/
	if (document.getElementById('criteria1').value == '') {
		document.getElementById('criteria1').value			= 'Search';
		document.getElementById('criteria1').style.color	= '#666';
		document.getElementById('formFocused1').value			= '0';		
	}
}

function mainSearchSubmit1(serverName) {
	
	if (!document.mainSearchForm1.criteria1.value || document.mainSearchForm1.formFocused1.value == '0') {
		alert('Your search entry contained no words; try again.');
		return false;
		}
	else
	{
		document.mainSearchForm1.action = 'http://search.usa.gov/search?affiliate=usmint.gov&query='+document.mainSearchForm.criteria.value;
		document.mainSearchForm1.submit();
		return true;
	}
	//
}

function mainSearchSubmitBtn1(serverName) {

	if (!document.mainSearchForm1.criteria1.value || !document.mainSearchForm1.formFocused1.value || document.mainSearchForm1.formFocused1.value == '0') {
		alert('Your search entry contained no words; try again.');
		}
	else
	{
	document.mainSearchForm1.action = 'http://search.usa.gov/search?affiliate=usmint.gov&query='+document.mainSearchForm1.criteria1.value;
	document.mainSearchForm1.submit();
	}
	//
}

</script>



	